{
  "env":
  {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "wesbos"
  ],
  "root": true,
  "parserOptions":
  {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["html"],
  "rules":
  {
    "no-console": 1,
    "no-unused-vars": [1,
    {
      "argsIgnorePattern": "res|next|^err"
    }],
    "arrow-body-style": [2, "as-needed"],
    "no-param-reassign": [2,
    {
      "props": false
    }],
    "import/prefer-default-export": 0,
    "quotes": 1,
    "no-restricted-globals": 1,
    "no-shadow": 1,
    "no-await-in-loop": 0,
    "no-restricted-syntax": 1,
    "no-use-before-define": 1,
    "no-promise-executor-return": 1,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120
      }
    ]
  }
}
